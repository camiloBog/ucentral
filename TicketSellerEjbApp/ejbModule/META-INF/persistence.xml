		<jta-data-source>jdbc/__PostgreLocal</jta-data-source>
		<class>com.model.entities.Aeropuerto</class>
		<class>com.model.entities.Avion</class>
		<class>com.model.entities.Banco</class>
		<class>com.model.entities.Ciudad</class>
		<class>com.model.entities.Clase</class>
		<class>com.model.entities.Estado</class>
		<class>com.model.entities.Pais</class>
		<class>com.model.entities.Perfil</class>
		<class>com.model.entities.Reserva</class>
		<class>com.model.entities.Sequence</class>
		<class>com.model.entities.Tarifa</class>
		<class>com.model.entities.Tipo</class>
		<class>com.model.entities.Usuario</class>
		<class>com.model.entities.Vuelo</class>
		<properties>
			<property name="eclipselink.target-database" value="PostgreSQL"/>
		</properties>
	</persistence-unit>
</persistence>
